{
    "welcomeFile": "index.html",
    "authenticationMethod": "none",
    "routes": [
        {
            "source": "/invoicesURL(.*)",
            "target": "$1",
            "destination": "invoicesDestination",
            "authenticationType": "none",
            "csrfProtection": false
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "authenticationType": "none",
            "localDir": "webapp"
        }
    ]
}